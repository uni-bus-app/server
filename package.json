    "rxjs-compat": "^6.5.4",
    "uopdf": "file:../UoPDF",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
